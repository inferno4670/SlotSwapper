{
  "version": 2,
  "github": {
    "silent": true
  },
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/build"
      }
    }
  ],
  "routes": [
    {
      "src": "^(?!/api/)/(.*)$",
      "dest": "/frontend/$1"
    }
  ]
}
